        android:usesCleartextTraffic="true">
        <activity
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"
            android:name=".WebViewActivity"></activity>
        <activity
            android:name=".ui_client.homescreen.ClientHomeActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name=".ui_seller.settingprofileediting.SettingProfileEditingActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"
            android:windowSoftInputMode="stateHidden|adjustPan" />
        <activity
            android:name=".ui_seller.shoplandingpage.ShopLandingActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name=".ui_seller.shopregistration.ShopRegistrationActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="${MAPS_API_KEY}" />

    </application>
